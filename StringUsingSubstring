package strings;

public class StringUsingSubstring {

	public static void main(String[] args) {
		String s = "ThisIsBombayCity";
		String s1 = s.substring(6);
		System.out.println("String from Substring : "+s1);
		String s2 = s.substring(6, 12);
		System.out.println("String from Substring : "+s2);
		

	}

}
