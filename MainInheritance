package inheritance;

public class Main {

	public static void main(String[] args) {
		System.out.println("===Class A===");
		A a = new A ();
		a.addition();
		a.subtraction();
		a.display();
		
		System.out.println("===Class B===");
		B b = new B();
		b.multiplication();
		b.division();
		b.display();
		
		System.out.println("===Class C===");
		C c = new C();
		c.statement();
		c.lang();
		c.display();
		
		System.out.println("Using data members");
		b.string();
		c.string();
		
		
		

	}

}
