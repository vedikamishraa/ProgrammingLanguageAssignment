package staticassignment;

public class StaticVariablesinInstanceMethod {
	
	int a;
	static int b;
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public static int getB() {
		return b;
	}
	public static void setB(int b) {
		StaticVariablesinInstanceMethod.b = b;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		StaticVariablesinInstanceMethod s = new StaticVariablesinInstanceMethod();
		s.setA(25);
		s.setB(63);
		System.out.println(s.getA());
		System.out.println(s.getB());
		
	}
}
