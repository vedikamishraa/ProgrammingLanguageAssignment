package interfaceassignment;

public class PublicClass implements Public {

	@Override
	public void add() {
		int sum = a+b;
		System.out.println(a);
		System.out.println(b);
		System.out.println(sum);
		
	}

	public static void main(String[] args) {
		PublicClass pc = new PublicClass();
		pc.add();
	}

}
