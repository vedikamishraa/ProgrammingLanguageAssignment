package staticassignment;

public class StaticandInstanceVariablesinMainMethod {
	int a =96;
	int b = 54;
	int sum=a+b;
	static String name="JAVA";
	
	public void add()
	{
		System.out.println(sum);
		
	}
	
	public static void main(String[] args) {
		StaticandInstanceVariablesinMainMethod s = new StaticandInstanceVariablesinMainMethod();
		s.add();
		System.out.println(name);
		
	
	}


}
