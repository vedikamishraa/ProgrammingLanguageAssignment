package methodoverloading;

public class MethodDifferentParameters {
	
	public void numbers(int a)
	{
		System.out.println("Number 1 : " +a);
	}
	
	public void numbers(int a, int b)
	{
		int sum = a+b;
		System.out.println(sum);
	}

	public static void main(String[] args) {
		MethodDifferentParameters md = new MethodDifferentParameters();
		md.numbers(258963);
		md.numbers(528, 789);
		

	}

}
